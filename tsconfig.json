{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "esModuleInterop": "true",
    "forceConsistentCasingInFileName": "true",
    "strict": "true",
    "moduleResolution": "Node",
    "skipLibCheck": "true"
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
